    "def GCD(m,n):\n",
    "    if m % n ==0:\n",
    "        return n \n",
    "    else:\n",
    "        return GCD(n,m % n)\n",
    "    \n",
    "print(\"84和1080的最大公因數為\",GCD(84,1080))"
