    "def fibo(n):\n",
    "    if n == 1 or n == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibo(n-1)+fibo(n-2)\n",
    "    \n",
    "for i in range(1,16):\n",
    "    print(fibo(i),end='\\t')"
   ]
