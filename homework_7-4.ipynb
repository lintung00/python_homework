    "def isLeapYear(year):\n",
    "    if(year%4==0 and year%100!=0)or(year%400==0):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "for i in range(2000,2051):\n",
    "    if isLeapYear(i):\n",
    "        print(i)"
