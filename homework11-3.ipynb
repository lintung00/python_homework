    "class Circle:\n",
    "     PI=3.14\n",
    "     radius=1\n",
    "        \n",
    "     def getArea(self):\n",
    "        return self.PI*self.radius*self.radius\n",
    "    \n",
    "C1=Circle()\n",
    "print(\"半徑為\",C1.radius,\"的圓面積為\",C1.getArea())\n",
    "\n",
    "C2=C1\n",
    "C2.radius=10\n",
    "print(\"半徑為\",C1.radius,\"的圓面積為\",C1.getArea())"
   ]
