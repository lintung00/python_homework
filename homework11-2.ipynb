   "source": [
    "try:\n",
    "    X=eval(input(\"請輸入被除數X:\"))\n",
    "    Y=eval(input(\"請輸入除數Y:\"))\n",
    "    Z=X/Y\n",
    "except ZeroDivisionError:\n",
    "    print(\"除數不得為0\")\n",
    "except Exception as e1:\n",
    "    print(e1.args)\n",
    "else:\n",
    "    print(\"沒有捕捉到例外!X除以Y的結果等於\",Z)\n",
    "finally:\n",
    "    print(\"離開try...except區塊\")"
   ]
